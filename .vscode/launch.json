    "version": "0.2.0",
    "configurations": [
        {
        "type": "node",
        "request": "launch",
        "name": "Debug NestJS",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run",
          "start:debug"
        ],
        "cwd": "${workspaceFolder}/backend",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "sourceMaps": true,
        "outFiles": [
          "${workspaceFolder}/backend/dist/**/*.js"
        ],
        "console": "integratedTerminal"
      }
    ]
}